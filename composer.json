{
    "name": "laravel/laravel",
    "version": "1.0.89",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "fakerphp/faker": "^1.15",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.1",
        "laravel/scout": "^9.3",
        "laravel/tinker": "^2.5",
        "phlak/semver-cli": "^1.0",
        "spatie/laravel-db-snapshots": "^1.7.1",
        "twoavy/evaluation-tool": "@dev",
        "ext-curl": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "laravel/sail": "v1.13.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "brainmaestro/composer-git-hooks": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Twoavy\\EvaluationTool\\Seeders\\": "packages/twoavy/evaluation-tool/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cghooks": "vendor/bin/cghooks",
        "semver": "vendor/bin/semver"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": [
                "echo committing as $(git config user.name)",
                "./vendor/bin/semver increment:patch",
                "git add ./composer.json",
                "echo pre commit done"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/twoavy/evaluation-tool",
            "options": {
                "symlink": true
            }
        }
    ]
}
